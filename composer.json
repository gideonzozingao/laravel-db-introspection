{
    "name": "zuqongtech/laravel-db-introspection",
    "description": "A Laravel package for automatic database introspection and model generation supporting MySQL, PostgreSQL, SQL Server, and more.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zuqongtech",
            "email": "support@zuqongtech.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Zuqongtech\\LaravelDbIntrospection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zuqongtech\\LaravelDbIntrospection\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zuqongtech\\LaravelDbIntrospection\\LaravelDbIntrospectionServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}